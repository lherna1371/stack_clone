<h1>Login</h1>
<%= current_user.id if current_user %>
<%= form_for :session, url: sessions_path do |f| %>
  <p>
    <%= f.label :handle %><br>
    <%= f.text_field :handle %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Login" %>
  </p>
<% end %>